{
  "expo": {
    "name": "hermit-app",
    "description": "A very interesting project.",
    "slug": "hermit-app",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/hermiticon2.png",
    "splash": {
      "image": "./assets/images/hermitsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "NSLocationWhenInUseUsageDescription": "Allow Hermit to user your location."
    }
  }
}
